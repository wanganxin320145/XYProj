    "strict": "global", //js文件顶部必须带"'use strict';"
    "freeze": true, //不允许修改原生对象的原型
    "node": true, //NodeJS环境（允许require等为全局变量）
    "browser": true, //浏览器环境（允许document等为全局变量）
